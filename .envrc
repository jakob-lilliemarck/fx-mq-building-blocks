#!/usr/bin/env bash

set -a
# Load workspace-level shared config first
source ../.env
# Override with crate-specific config
if [ -f .env ]; then
    source .env
fi
if [ -f .env.local ]; then
    source .env.local
fi
# Override sqlx offline mode for development in this crate
export SQLX_OFFLINE=false
set +a
